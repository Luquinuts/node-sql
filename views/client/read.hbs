<table class="container">
    <tr class="header-row">
        <td class="col">#ID</td>
        <td class="col">Nombre Completo</td>
        <td class="col">Fecha Nacimiento</td>
        <td class="col">Peso</td>
        <td class="col">Altura</td>
        <td class="col">Email</td>
        <td class="col">Movil</td>
    </tr>
    {{#each data}}
    <tr class="row">
        <td class="col">{{idClientes}}</td>
        <td class="col">{{NombreCliente}} {{ApellidoCliente}}</td>
        <td class="col">{{toFormat FechaNacCliente}}</td>
        <td class="col">{{PesoCliente}}</td>
        <td class="col">{{AlturaCliente}}</td>
        <td class="col">{{EmailCliente}}</td>
        <td class="col">{{Movil01Cliente}}</td>
    </tr>
    {{/each}}
</table>